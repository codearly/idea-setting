<application>
  <component name="ProjectManager">
    <defaultProject>
      <component name="MavenImportPreferences">
        <option name="generalSettings">
          <MavenGeneralSettings>
            <option name="mavenHome" value="$USER_HOME$/Environment/apache-maven-3.6.3" />
            <option name="userSettingsFile" value="$USER_HOME$/Environment/apache-maven-3.6.3/conf/settings.xml" />
          </MavenGeneralSettings>
        </option>
      </component>
      <component name="PropertiesComponent">
        <property name="settings.editor.selected.configurable" value="MavenSettings" />
      </component>
    </defaultProject>
  </component>
</application>